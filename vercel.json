{
  "rewrites": [
    { "source": "/(.*)", "destination": "/GenZSmart/$1" }
  ],
  "redirects": [
    { "source": "/", "destination": "/GenZSmart/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}

